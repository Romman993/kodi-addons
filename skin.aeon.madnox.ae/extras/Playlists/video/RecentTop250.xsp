<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>RecentTop250</name>
    <match>all</match>
    <rule field="playlist" operator="is">
        <value>IMDB Top 250</value>
    </rule>
    <limit>25</limit>
    <order direction="descending">dateadded</order>
</smartplaylist>
